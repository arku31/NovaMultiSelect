{
    "name": "arku/nova-multiselect",
    "description": "A Laravel Nova Multiselect Implementation.",
    "keywords": [
        "laravel",
        "nova",
        "multiselect"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Nova\\Multiselect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nova\\Multiselect\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
